    "pm2": "^4.4.0",
    "postgraphile": "^4.6.0",
    "postgraphile-nest": "^1.0.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.4",
    "sequelize": "^5.21.7",
    "sequelize-typescript": "^1.1.0",
    "swagger-ui-express": "^4.1.3",
