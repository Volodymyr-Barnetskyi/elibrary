    "@nestjs/jwt": "^6.1.2",
    "@nestjs/passport": "^6.2.0",
    "@nestjs/platform-express": "^6.11.11",
    "@nestjs/sequelize": "^0.1.0",
    "@nestjs/swagger": "^4.5.3",
    "@nestjs/typeorm": "^6.1.2",
    "@types/graphql": "^14.5.0",
    "apollo-server-express": "^2.12.0",
    "aws-lambda": "^1.0.6",
    "aws-serverless-express": "^3.3.8",
    "dotenv": "^8.2.0",
